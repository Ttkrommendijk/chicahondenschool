{
  "json.schemaDownload.enable": false,
  "json.schemas": [
    {
      "fileMatch": [
        "package.json"
      ],
      "url": "./.vscode/schemas/package.schema.json"
    },
    {
      "fileMatch": [
        ".eslintrc",
        ".eslintrc.json"
      ],
      "url": "./.vscode/schemas/eslintrc.schema.json"
    },
    {
      "fileMatch": [
        ".jscpd.json",
        "jscpd.json"
      ],
      "url": "./.vscode/schemas/jscpd.schema.json"
    }
  ],
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore"
}
